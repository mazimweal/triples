{
    "name": "tdt/triples",
    "description": "",
    "authors": [
        {
            "name": "Jan Vansteenlandt",
            "email": "jan@okfn.be"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.18",
        "semsol/arc2" : "dev-master",
        "easyrdf/easyrdf" : "0.9.1"
    },
    "require-dev" : {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/models"
        ],
        "psr-0": {
            "Tdt\\Triples\\": "src/"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-install-cmd": [
            "php artisan optimize",
            "php artisan asset:publish tdt/triples"
        ],
        "post-update-cmd": [
            "php artisan optimize",
            "php artisan asset:publish tdt/triples"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
